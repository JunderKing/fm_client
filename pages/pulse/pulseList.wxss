view.item_list{
    padding: 10px 10px 0;
}
navigator.item_box{
    padding: 10px 0;
    border-top: 1px solid #dcdcdc;
    font-size: 16px;
    position: relative;
}
navigator.item_box>text.content{
    position: absolute;
    left: 60px;
    top: 10px;
    font-size: 16px;
}
view.text_box{
    text-align: center;
    font-size: 30px;
    border: 1px solid #c0c0c0;
    border-radius: 4px;
    width: 50px;
    height: 50px;
    line-height: 50px;
    color: #808080;
}
text.pulse_content{
    font-size: 14px;
    line-height: 16px;
    color: #353535;
}


