/* pages/canvas/canvas.wxss */
view.canvas_row{
    width: 100vw;
    height: 25vh;
}
view.canvas_subtitle{
    box-sizing: border-box;
    width: 100%;
    height: 20px;
    /*padding-top: 5px;*/
    font-size: 14px;
    line-height: 20px;
    text-align: center;
    background-color: #f0f0f0;
    color: #888;
}
navigator.canvas_box{
    display: inline-block;
    box-sizing: border-box;
    width: 50%;
    height: calc(100% - 20px);
    line-height: calc(25vh - 20px);
    text-align: center;
    border: 1px solid #f0f0f0;
    font-size: 25px;
    position: relative;
    background-color: #fff;
}
